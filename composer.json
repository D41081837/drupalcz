{
    "name": "Drupal.cz",
    "description": "Drupal.cz Community website",
    "type": "project",
    "license": "GPL-2.0+",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.6",
        "drupal/admin_toolbar": "^1.19",
        "drupal/bootstrap": "^3.5",
        "drupal/bootstrap_layouts": "5.x-dev",
        "drupal/config_split": "^1.0@beta",
        "drupal/config_update": "^1.3",
        "drupal/console": "~1.0",
        "drupal/core": "~8.0",
        "drupal/ctools": "^3.0",
        "drupal/devel": "^1.0@RC",
        "drupal/dropguard": "^1.1",
        "drupal/ds": "^3.1",
        "drupal/entity_reference_revisions": "^1.3",
        "drupal/features": "^3.5",
        "drupal/geolocation": "^1.10",
        "drupal/google_analytics": "^2.1",
        "drupal/jssor": "^1.0@RC",
        "drupal/juicebox": "^2.0@beta",
        "drupal/layout_plugin": "^1.0@alpha",
        "drupal/libraries": "3.x-dev",
        "drupal/migrate_plus": "^4.0",
        "drupal/migrate_tools": "^4.0",
        "drupal/mollom": "^1.2",
        "drupal/panelizer": "^4.0",
        "drupal/panels": "^4.1",
        "drupal/paragraphs": "^1.1",
        "drupal/pathauto": "^1.0",
        "drupal/token": "^1.0",
        "drupal/vardumper": "^1.3",
        "drupal/youtube": "^1.0@beta",
        "drush/drush": "^8.1",
        "webflo/drupal-finder": "^0.3.0",
        "webmozart/path-util": "^2.3"
    },
    "require-dev": {
        "behat/mink": "~1.7",
        "behat/mink-goutte-driver": "~1.2",
        "jcalderonzumba/gastonjs": "~1.0.2",
        "jcalderonzumba/mink-phantomjs-driver": "~0.3.1",
        "mikey179/vfsstream": "~1.2",
        "phpunit/phpunit": ">=4.8.28 <5",
        "symfony/css-selector": "~2.8"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
      "sort-packages": true
    },
    "extra": {
        "installer-paths": {
            "docroot/core": ["type:drupal-core"],
            "docroot/libraries/{$name}": ["type:drupal-library"],
            "docroot/modules/contrib/{$name}": ["type:drupal-module"],
            "docroot/profiles/contrib/{$name}": ["type:drupal-profile"],
            "docroot/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/contrib/{$name}": ["type:drupal-drush"]
        },
        "patches": {
            "drupal/jssor": {
                "Undefined index slide_spacing": "https://www.drupal.org/files/issues/warning_slide_spacing-2673660-1.patch"
            },
            "drupal/layout_plugin": {
                "Cannot use object of type": "https://www.drupal.org/files/issues/fatal-error-when-installing-module.patch"
            }
        }
    }
}
